{
  "name": "calculator_fullstack",
  "version": "1.0.0",
  "description": "full stack simple calculator webapp",
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "build": "npm run build -ws",
    "start": "npm start -w backend",
    "dev": "concurrently \"npm run dev -w backend\" \"npm start -w frontend\"",
    "lint": "npm run lint -ws",
    "test": "npm run test:cov -w backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maakhu/calculator_fullstack.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "author": "Mate Egri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maakhu/calculator_fullstack/issues"
  },
  "homepage": "https://github.com/maakhu/calculator_fullstack#readme"
}
